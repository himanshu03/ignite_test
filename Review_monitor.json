{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://ignite.where2stageit.com/login.html"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "loginid"
      },
      "text": "qa_tester"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "Where2getit!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "b"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "accounts_chzn_o_0"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "btn-select-account"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img[alt=\"Reputation\"]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@id='SourceSelection']/li[1]/span"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@id='SourceSelection']//span[normalize-space(.)='Yelp']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@id='SourceSelection']/li[3]/span"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@id='SourceSelection']/li[4]/span"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@id='SourceSelection']/li[5]/span"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Select a location:"
    },
    {
      "type": "verifyTextPresent",
      "text": "Reviewer"
    },
    {
      "type": "verifyTextPresent",
      "text": "Location Name"
    },
    {
      "type": "verifyTextPresent",
      "text": "City & State"
    },
    {
      "type": "verifyTextPresent",
      "text": "Review"
    },
    {
      "type": "verifyTextPresent",
      "text": "Rating"
    },
    {
      "type": "verifyTextPresent",
      "text": "Review Date"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Back to Launchpad"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "logout-button"
      }
    }
  ]
}